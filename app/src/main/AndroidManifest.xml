<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ha.tube">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="tube"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreatorActivity"
            android:label="@string/title_activity_creator"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ViewerActivity"
            android:label="@string/title_activity_viewer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".YoutuberActivity"
            android:label="건국대학교"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AddActivity"></activity>
    </application>

</manifest>